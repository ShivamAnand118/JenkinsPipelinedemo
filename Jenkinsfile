pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello its Deploying'
            }
        }
        stage('Testing') {
            steps {
                echo 'Hello time to test'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello Completed'
            }
        }
    }
}
